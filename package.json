{
  "name": "node-service-template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "npm run clean && babel ./ --out-dir ./build --presets es2015 --ignore ./node_modules,./build",
    "build:watch": "npm run build -- --watch",
    "clean": "rimraf ./build",
    "lint": "eslint ./ --ignore-pattern build/",
    "start": "cross-env NODE_ENV=production PORT=80 node ./build/bin/www",
    "start:dev": "cross-env DEBUG=skeleton:* nodemon ./build/bin/www.js --watch ./build/",
    "test": "cross-env mocha --recursive ./build/test",
    "test:debug": "cross-env DEBUG=skeleton:* mocha --recursive ./build/test"
  },
  "dependencies": {
    "body-parser": "1.15.0",
    "cross-env": "1.0.7",
    "debug": "2.2.0",
    "express": "4.13.4",
    "jsonwebtoken": "5.7.0",
    "lodash": "4.5.1",
    "mongoose": "4.4.5",
    "morgan": "1.7.0",
    "node-cache": "3.2.0",
    "passport": "0.3.2",
    "passport-http-bearer": "1.0.1"
  },
  "devDependencies": {
    "babel-cli": "6.5.1",
    "babel-preset-es2015": "6.5.0",
    "babel-register": "6.5.2",
    "chai": "3.5.0",
    "eslint": "2.2.0",
    "eslint-config-airbnb": "6.0.2",
    "eslint-plugin-react": "4.1.0",
    "mocha": "2.4.5",
    "nodemon": "1.9.0",
    "npm-check-updates": "2.5.8",
    "request": "2.69.0",
    "rimraf": "2.5.2"
  }
}
